@model NGO.Models.contact
@{
    ViewBag.Title = "Contact";
    Layout = "~/Views/Shared/_Layout.cshtml";
}





<!--  ************************* Contact Us Starts Here ************************** -->


<div style="margin-top:0px;" class="row no-margin">

    <iframe style="width:100%" src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3617.7978330696296!2d67.04237307424961!3d24.93895614211425!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3eb33f3daae54d55%3A0x5dae2a3a4b71f12a!2sAptech%20Learning%20North%20Nazimabad%20II%20Center!5e0!3m2!1sen!2s!4v1691480807084!5m2!1sen!2s" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>


</div>

<div class="row contact-rooo no-margin">
    <div class="container">
        <div class="row">

            @using (Html.BeginForm("Contact", "users", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {

                <div class="container" style="width:400px;">
                    <h1 style="margin-top:20px; margin-left:100px;">Feedback</h1>

                    @Html.TextBoxFor(x => x.c_name, new { @class = "form-control", placeholder = "Enter Name" })
                    @Html.TextBoxFor(x => x.c_email, new { @class = "form-control", placeholder = "Enter Email" })
                    @Html.TextBoxFor(x => x.c_phone, new { @class = "form-control", placeholder = "Enter phone" })
                    @Html.TextAreaFor(x => x.c_feedback, new { @class = "form-control", placeholder = "Enter Message..." })





                    <input style="margin-left: 150px;" type="submit" value="submit" name="submit" class="btn btn-success" />
                    <br />
                    <br />

                </div>
            }

            <div class="col-sm-5"  style="margin-left:220px;">

                <div style="margin:50px" class="serv">





                    <h2 style="margin-top:10px;">Address</h2>

                    NGO charitables <br>
                    Aptech NN2 <br>
                    North Nazimabad<br>
                    Phone:+91 9159669599<br>
                    Email:info@NGOCharitables.com<br>
                    Web: <a href="smart-eye.html" class="">www.NGOCharitables.com</a>







                </div>


            </div>

        </div>
    </div>

</div>